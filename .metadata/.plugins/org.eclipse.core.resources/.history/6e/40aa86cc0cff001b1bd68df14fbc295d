#include <iostream>
using namespace std;

class Employee{
	string Name;
	string Company;
	int Age;
	void introduceYourself(){
		cout << "Name - " << Name << endl;
		cout << "Company " << Company << endl;
		cout << "Age - " << Age << endl;

	}
	Employee(){

	}
};
int main(){
	Employee employee1;
	employee1.Name = "Nathan";
	employee1.Company = "Autonomous Research Corporation";
	employee1.Age = "26";

	return 0;

}
