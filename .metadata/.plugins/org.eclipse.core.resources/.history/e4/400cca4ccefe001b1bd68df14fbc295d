/*
 * endl tells c++ to break the line and write on a new one
 *
 *
 */
void maleTest(bool male);
int main() {
	bool isMale = true;

	maleTest(isMale);

	return 0;
}
void maleTest(bool male){
	if(male){
			cout << "You are a male";
		} else {
			cout << "You are not a male";
		}
}


